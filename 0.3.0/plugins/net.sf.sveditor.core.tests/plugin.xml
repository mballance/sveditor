<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="testSVScannerLineNumbers"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="net.sf.sveditor.core.tests.testSVScannerLineNumbers">
         </run>
      </application>
   </extension>
   <extension
         id="testPreProcessor"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="net.sf.sveditor.core.tests.testPreProcessor">
         </run>
      </application>
   </extension>
   <extension
         id="testPersistence"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="net.sf.sveditor.core.tests.SVDBPersistence">
         </run>
      </application>
   </extension>
   <extension
         point="net.sf.sveditor.core.SVLibraries">
      <SVLibrary
            default="false"
            id="org.ovmworld.ovm"
            name="OVM Library"
            path="ovm/src/ovm_pkg.sv">
      </SVLibrary>
      <SVLibrary
            default="false"
            id="org.vmmcentral.vmm"
            name="VMM Library"
            path="vmm/sv/vmm_lib.sv">
      </SVLibrary>
   </extension>

</plugin>
